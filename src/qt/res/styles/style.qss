        border-width: 0;
        border-style: none;
        padding: 8px;
        background-color: #001849;
        color: white;
}

QFormLayout {
        border-radius: 0px;
        border-width: 0;
        border-style: none;
        padding: 8px;
        background-color: #001849;
        color: white;
}

QSpinBox {
        border-radius: 0px;
        border-width: 0;
        border-style: none;
        padding: 8px;
        background-color: #001849;
        color: white;
}

QCheckBox {
        border-radius: 0px;
        border-width: 0;
        border-style: none;
        padding: 8px;
        background-color: #001849;
