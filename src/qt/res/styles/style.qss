QProgressBar {
        background-color: #111111;
        border-radius: 0px;
        text-align: center;
        color: white;
}

QProgressBar::chunk {
        background: QLinearGradient(x1: 0, y1: 0, x2: 1, y2: 0, stop: 0 #ff0000, stop: 1 #961900);
        border-radius: 0px; margin: 0px;
}

QScrollArea {
